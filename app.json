{
  "expo": {
    "name": "Plan ZS2",
    "slug": "zs2-aplikacja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "zs2plan",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#16396E"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#16396E"
      },
      "package": "com.zs2fum.plan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#1e4d95"
              },
              "textColorPrimary": {
                "light": "#000000",
                "dark": "#FFFFFF"
              },
              "textColorSecondary": {
                "light": "#000000",
                "dark": "#FFFFFF"
              },
              "textColorPrimaryInverse": {
                "light": "#FFFFFF"
              },
              "textColorSecondaryInverse": {
                "light": "#FFFFFF"
              },
              "windowBackground": {
                "light": "#FFFFFF",
                "dark": "#111827"
              },
              "textColor": {
                "light": "#000000",
                "dark": "#FFFFFF"
              }
            }
          }
        }
      ]
    ]
  }
}
